#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include <time.h>

void insertionSort(int v[], int n){
    for(int i=0; i<n; i++){
        int key = v[i];
        int j = i-1;

        while(j>=0 && v[j] > key){
            v[j+1] = v[j];
            j = j-1;
        }

        v[j+1] = key;
    }
}

int main(){
    int n = 5;
    int v[5] = {0}; 

    
    clock_t start = clock();

    insertionSort(v, n); 

    clock_t end = clock();

    double tmp = (double)(end - start) / CLOCKS_PER_SEC;

    printf("Execution time: %f seconds\n", tmp);

    return 0;

}
